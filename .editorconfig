[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 4
tab_width = 4

[*.{yml,yaml}]
indent_size = 2

[*.md]
indent_size = 2
trim_trailing_whitespace = false

[*.{csproj,xml,props}]
indent_size = 2

[*.cs]
dotnet_style_prefer_collection_expression = false:none